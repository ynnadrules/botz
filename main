#!/usr/bin/env ruby

$:.unshift(File.dirname(__FILE__) + '/lib') unless $LOAD_PATH.include?(File.dirname(__FILE__) + '/lib')

require 'pry'
require 'bot'
require 'logic'

Botz::Bot.configure do |b|

  b.base_uri b.api_endpoint

  b.player_key = "22e44c2c-b4ed-4354-95a2-e83727f91d1e"

end

Botz::Bot.play do
  move = Logic.new.execute game_state

  make_play move
end


